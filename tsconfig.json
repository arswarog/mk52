{
  "compileOnSave": true,
  "compilerOptions": {
    "outDir": "./.tmp",
    "sourceMap": true,
    "declaration": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "jsx": "react",
    "typeRoots": [
      "node_modules/@types",
      "types"
    ],
    "lib": [
      "es2015",
      "dom"
    ]
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "src/**/*.spec.ts",
    "src/emk/getCommands.ts"
  ],
  "externals": {
    "fs": "commonjs fs",
    "path": "commonjs path"
  }
}
